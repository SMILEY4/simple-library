    "electron-log": "^4.3.1",
    "electron-store": "^7.0.0",
    "fast-xml-parser": "^3.20.3",
    "htmlparser2": "^7.1.2",
    "node-exiftool": "^2.3.0",
    "react": "^16.13.1",
    "react-contexify": "^5.0.0",
    "react-datepicker": "^4.2.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.2.0",
    "react-popper": "^2.2.5",
