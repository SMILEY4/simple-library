{
  "name": "simple-library",
  "main": "./.webpack/main/bundle.js",
  "version": "0.2.0",
  "description": "A simple digital asset library.",
  "author": "Lukas Ruegner",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "webpack --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "package": "npm run build && electron-builder --dir .",
    "dist": "npm run build && electron-builder .",
    "start": "concurrently \"npm run start:dev\" \"npm run start:electron\"",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js",
    "start:electron": "npm run build:dev && electron ./.webpack/main/bundle.js"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/sqlite3": "^3.1.6",
    "dist-exiftool": "^10.53.0",
    "electron-log": "^4.3.1",
    "electron-store": "^7.0.0",
    "node-exiftool": "^2.3.0",
    "react": "^16.13.1",
    "react-contexify": "^5.0.0",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-icons": "^4.2.0",
    "react-split-pane": "^0.1.92",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.27.1",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "electron": "^11.1.1",
    "electron-builder": "^22.4.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.0.4",
    "prettier": "^2.0.2",
    "raw-loader": "^4.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2"
  }
}
